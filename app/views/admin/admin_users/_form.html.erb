<%= form_for([:admin, @admin_user]) do |f| %>
	<%= render :partial =>  'shared/errors', :locals => {:model => @admin_user } %>

  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
