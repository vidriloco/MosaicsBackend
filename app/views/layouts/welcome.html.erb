<!DOCTYPE html>
<html>
	<head>
	  <title>Backend</title>
	  <%= stylesheet_link_tag    "application" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	</head>
<body>

	<div id="header">
		<%= yield :header %>
	</div>
	<div id="notifications"><%= yield :notifications %></div>

	<div id="content">
		<%= yield :body %>
	</div>

	<div id="footer">
		<%= yield :footer %>
	</div>

</body>
</html>